<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/activity_serial_console"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="go.upsseriallogger.SerialConsoleActivity">
   >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp">

        <Button
            android:text="ESCH"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:id="@+id/ESCH"
            android:onClick="ESCH_click" />

        <Button
            android:text="ESCL"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:id="@+id/ESCL"
            android:onClick="ESCL_click" />

        <Button
            android:text="ESCB"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:id="@+id/ESCB"
            android:onClick="ESCB_click" />

        <Button
            android:text="ESCN"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:id="@+id/ESCN"
            android:onClick="ESCN_click" />

        <Button
            android:text="clear"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:id="@+id/clearlist"
            android:onClick="clearlist_click" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/listview3" />
    </FrameLayout>
</LinearLayout>
